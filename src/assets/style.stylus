#cellTable,
#columnTable {
  tr {
    td:nth-child(1) {
      padding-top: 15px
    }
    th,td {
      text-align: center
      vertical-align: middle;
    }
  }
}
.form-control.no-validate:valid
    border-color #ced4da !important
    padding-right .75rem !important
    background-image none !important